Cell[BoxData[{
 RowBox[{
  RowBox[{"directoryGraph", "[", "a_Association", "]"}], " ", ":=", 
  RowBox[{"Join", " ", "@@", " ", 
   RowBox[{"KeyValueMap", "[", 
    RowBox[{"makeGraph", ",", " ", "a"}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"directoryGraph", "[", 
   RowBox[{"s_String", ",", " ", "a_Association"}], "]"}], " ", ":=", " ", 
  RowBox[{"Join", "[", 
   RowBox[{
    RowBox[{"Thread", "[", 
     RowBox[{"s", " ", "\[Rule]", " ", 
      RowBox[{"Keys", "[", "a", "]"}]}], "]"}], ",", " ", 
    RowBox[{"makeGraph", "[", "a", "]"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"directoryGraph", "[", 
